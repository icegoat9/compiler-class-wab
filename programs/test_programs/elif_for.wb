// test nested elif and for structures
//  (both get rewritten to other structures in early compiler passes)

for i = 1; i < 10; i = i + 1; {
    if i <= 3 {
        print i;
    }
    elif i <= 6 {
        for j=1;j<4;j=j+1; {
            print (10*i)+j;
        }
    }
    else {
        print 100*i;
    }
}
