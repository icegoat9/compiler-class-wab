//
// power function x ^ n
// test case for function with multiple arguments
//   (bug w/ argument order on stack in previous compiler)

func pow(x, n) {
    var i = 0;
    var result = 1;
    while i < n {
        result = result * x;
        i = i + 1;
    }
    return result;
}

print 3;
print 4;
print pow(3, 4);   // should be 3^4 = 81




